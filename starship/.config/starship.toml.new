style = "bg:fg_reverse fg:orange"
format = '[î¶(fg:fg_reverse)$symbol($version)î¶(fg:fg_reverse)($style)'

[docker_context
symbol = " "
style = "bg:bg_light fg:blue"
format = '[î¶(fg:bg_light)$symbol$contextî¶(fg:bg_light)($style)'
only_with_files = true

[kubernetes
symbol = "󱃾 "
style = "bg:bg_light fg:cyan"
format = '[î¶(fg:bg_light)$symbol$context( \($namespace\))î¶(fg:bg_light)($style)'
disabled = false

[terraform
symbol = "󱁢 "
style = "bg:bg_light fg:violet"
format = '[î¶(fg:bg_light)$symbol$workspaceî¶(fg:bg_light)($style)'

[cmd_duration
min_time = 500
format = "[î¶(fg:yellow)ó±¦ $durationî¶(fg:yellow)(fg:bg_dark bg:yellow)"
