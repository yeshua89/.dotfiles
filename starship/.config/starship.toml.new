format = '[î¶(fg:fg_reverse)$symbol($version)( $virtualenv)î¶(fg:fg_reverse)($style)'
detect_extensions = ['py'
detect_files = ['.python-version', 'Pipfile', '__pycache__', 'pyproject.toml', 'requirements.txt', 'setup.py', 'tox.ini'

[java
symbol = " "
style = "bg:fg_reverse fg:red"
format = '[î¶(fg:fg_reverse)$symbol($version)î¶(fg:fg_reverse)($style)'

[kotlin
