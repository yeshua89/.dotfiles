# Enhanced version with better colors, more languages, and improved features

"$schema" = 'https://starship.rs/config-schema.json'

format = """
(bg:bg)\
$os\
$username\
(fg:bg)\
(bg:bg_visual)\
$directory\
(fg:bg_visual)\
(bg:bg_light)\
$git_branch\
$git_status\
(fg:bg_light)\
$c\
$cmake\
$rust\
$golang\
$nodejs\
$php\
$python\
$java\
$kotlin\
$lua\
$ruby\
$zig\
$docker_context\
$kubernetes\
$terraform\
$cmd_duration\
$jobs\
$status\
$line_break\
$character"""

palette = 'tokyo_night'

[palettes.tokyo_night
bg_dark = '#16161e'
bg = '#1a1b26'
bg_light = '#24283b'
bg_visual = '#2f3549'
fg = '#c0caf5'
fg_dim = '#565f89'
fg_reverse = '#1a1b26'
red = '#f7768e'
orange = '#ff9e64'
yellow = '#e0af68'
green = '#9ece6a'
cyan = '#7dcfff'
blue = '#7aa2f7'
violet = '#bb9af7'
magenta = '#bb9af7'

[os
disabled = false
style = "bg:bg fg:cyan"
format = "[$symbol(bg:bg fg:cyan)"

[os.symbols
Alpine = " "
Amazon = " "
Android = " "
Arch = "󰣇 "
Artix = "󰣇 "
CentOS = " "
Debian = "󰣚 "
Fedora = "󰣛 "
Gentoo = "󰣨 "
Linux = "󰌽 "
Macos = " "
Manjaro = " "
Mint = "󰣭 "
NixOS = " "
Raspbian = "󰐿 "
Redhat = "󱄛 "
RedHatEnterprise = "󱄛 "
SUSE = " "
Ubuntu = "󰕈 "
Unknown = " "
Windows = "󰍲 "

[username
show_always = true
style_user = "bg:bg fg:blue"
style_root = "bg:bg fg:red bold"
format = '[$user (bg:bg $style)'

[directory
style = "fg:fg bg:bg_visual bold"
format = "[ $path (bg:bg_visual $style)"
truncation_length = 4
truncation_symbol = "…/"
truncate_to_repo = true
read_only = " 󰌾"
read_only_style = "fg:red bg:bg_visual"

[directory.substitutions
"Documents" = "󰈙 "
"Downloads" = " "
"Music" = "󰝚 "
"Pictures" = " "
"Videos" = " "
"Developer" = "󰲋 "
"Projects" = " "
".config" = " "
"Desktop" = "󰚝 "

[git_branch
symbol = " "
style = "bg:bg_light fg:violet bold"
format = '[ $symbol$branch (bg:bg_light $style)'
truncation_length = 20
truncation_symbol = "…"

[git_status
style = "bg:bg_light fg:yellow"
format = '[($all_status$ahead_behind )(bg:bg_light $style)'
conflicted = "󰞇"
ahead = "󰜷${count}"
behind = "󰜮${count}"
diverged = "󰃻${ahead_count}󰃻${behind_count}"
untracked = "?"
stashed = "󰜦"
modified = "󱇨"
staged = "󰄬${count}"
renamed = "󰑕"
deleted = "󰍵"

[c
symbol = " "
style = "bg:fg_reverse fg:cyan"
format = '[î¶(fg:fg_reverse)$symbol($version)î¶(fg:fg_reverse)($style)'

[cmake
symbol = "△ "
style = "bg:fg_reverse fg:cyan"
format = '[î¶(fg:fg_reverse)$symbol($version)î¶(fg:fg_reverse)($style)'

[rust
symbol = " "
style = "bg:fg_reverse fg:orange"
format = '[î¶(fg:fg_reverse)$symbol($version)î¶(fg:fg_reverse)($style)'

[golang
symbol = " "
style = "bg:fg_reverse fg:cyan"
format = '[î¶(fg:fg_reverse)$symbol($version)î¶(fg:fg_reverse)($style)'

[nodejs
