format = "[î¶(fg:blue)$symbol$numberî¶(fg:blue)($style)"

[status
disabled = false
format = '[î¶(fg:red)$symbol$statusî¶(fg:red)(fg:bg_dark bg:red bold)'
symbol = "✗ "
success_symbol = ""
not_executable_symbol = "󰌋 "
not_found_symbol = "󰍉 "
sigint_symbol = "󰂭 "
signal_symbol = "󱐋 "

[line_break
disabled = false

